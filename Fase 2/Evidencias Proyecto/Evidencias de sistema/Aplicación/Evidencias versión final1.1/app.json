{
  "expo": {
    "name": "Avijuelas",
    "slug": "avijuelas",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "permissions": [],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.tuempresa.avijuelas",
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "Necesitamos acceso a tu galería de fotos para seleccionar una imagen.",
        "NSCameraUsageDescription": "Necesitamos acceso a tu cámara para tomar fotos.",
        "NSLocationWhenInUseUsageDescription": "Necesitamos acceder a tu ubicación para mostrar el mapa correctamente.",
        "NSLocationAlwaysUsageDescription": "Necesitamos acceso a tu ubicación para mostrar el mapa correctamente."
      }
    },
    "android": {
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "ACCESS_FINE_LOCATION"
      ],
      "package": "com.tuempresa.avijuelas",
      "usesCleartextTraffic": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "scripts": {
      "start": "node server.js"
    },
    "dependencies": {
      "express": "^4.18.2",
      "mysql2": "^3.2.0",
      "body-parser": "^1.20.2",
      "bcrypt": "^5.1.0",
      "react-native-maps": "0.31.1"
    },
    "extra": {
      "eas": {
        "projectId": "c8c19dd1-c88e-4153-9419-2283fb13d09d"
      }
    },
    "owner": "pravijuelas1"
  }
}
